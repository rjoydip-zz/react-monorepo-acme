{
  "name": "@react-svgjs/monorepo",
  "version": "0.0.1",
  "description": "React SVG.js",
  "private": true,
  "engines": {
    "node": ">=14 <=16"
  },
  "sideEffects": false,
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "prepare": "husky install && chmod ug+x .husky/*",
    "typecheck": "tsc -p ./tsconfig.base.json --noEmit",
    "lint": "eslint packages/**/*.{ts,tsx} --max-warnings 0",
    "lint:fix": "eslint packages/**/*.{ts,tsx} --max-warnings 0 --fix",
    "format": "prettier --write '!{coverage,example,build,.cache}/**/*' '**/*.{md,json,ts,tsx}'",
    "build": "run-p clean typecheck format lint:fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "clean": "rimraf dist/ build/ coverage/ .parcel-*/ cache/ .pnpm-* *-debug.log *.lock *-lock.*",
    "predeploy": "cd example && pnpm install && pnpm build",
    "deploy": "gh-pages -d example/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/react-svgjs.git"
  },
  "keywords": [],
  "author": "rjoydip",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjoydip/react-svgjs/issues"
  },
  "homepage": "https://github.com/rjoydip/react-svgjs#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitest/ui": "^0.17.0",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vitest": "^0.17.0"
  },
  "lint-staged": {
    "**/*.{md,json,ts,tsx}": "prettier --write"
  },
  "pnpm": {
    "overrides": {}
  },
  "dependencies": {
    "preact": "^10.9.0"
  }
}
